[repository]
native = LF

[patterns]
# Mercurial settings files.
.hgignore = native
.hgtags = native
.hgeol = native

# chromiumembedded eol-style
third_party/chromiumembedded/include/**.h = LF
third_party/chromiumembedded/version.h = native

# Source and project files.
**.cs = native
**.csproj = native
**.proj = native
**.py = native
**.tt = native
**.ttinclude = native
**.sln = CRLF
**.h = native
**.c = native
**.cc = native
**.cpp = native

# Shell scripts
**.sh = native
**.cmd = native

# Resources
**.html = native
